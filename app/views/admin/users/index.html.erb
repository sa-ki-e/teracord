<h1>会員一覧</h1>
<div>
<div class='container px-5 px-sm-0'>
<table class='table table-hover table-inverse'>
 <thead>
   <tr>
     <th>ユーザーID</th>
     <th>名前</th>
     <th>メールアドレス</th>
     <th>ステータス</th>
     <th colspan="4"></th>
   </tr>
 </thead>
<% @users.each do |user| %>
 <tbody>
<tr>
  <td>
    <%= user.id %>
  </td>
  <td>
    <%= link_to admin_user_path(user.id) do %>
    <%= user.name %>
    <% end %>
  </td>
  <td>
    <%= user.email %>
  </td>
  <td>
    <%= user.is_deleted? ? "退会" : "有効" %>
  </td>
  <% end %>
</tr>
</tbody>
</table>
<%#= paginate @users %>
</div>
</div>